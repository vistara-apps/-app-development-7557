# Phyght Combat Video Platform Environment Variables

# Stealth Mode Configuration
# Set to 'true' to hide all premium/token features from users
VITE_STEALTH_MODE=true

# API Configuration
VITE_API_BASE_URL=http://localhost:3001
VITE_VIDEO_CDN_URL=https://cdn.phyght.com

# Wallet Configuration
VITE_WALLET_CONNECT_PROJECT_ID=your_wallet_connect_project_id
VITE_ALCHEMY_API_KEY=your_alchemy_api_key

# Analytics
VITE_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX
VITE_MIXPANEL_TOKEN=your_mixpanel_token

# Video Services
VITE_CLOUDFLARE_STREAM_TOKEN=your_cloudflare_stream_token
VITE_AWS_S3_BUCKET=your_s3_bucket_name

# Admin Configuration
VITE_ADMIN_SECRET_KEY=your_admin_secret_key

# Development
VITE_DEBUG_MODE=false
VITE_MOCK_DATA=true

